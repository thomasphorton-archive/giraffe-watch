a(href='javascript:void()').btn.btn-danger.btn-lg.btn-block#giraffe-event Report Active Labor

p#giraffe-event-result Thanks for the heads up! We'll check it out shortly.

script.
  document.getElementById('giraffe-event').onclick = function(e) {
    e.preventDefault();
    var r = prompt('Is April in Active Labor? This will send an email to tell us to check April. Enter \'Yes\' to continue. THIS DOES NOT SUBSCRIBE YOU TO THE NOTIFICATIONS!');
    if (r && r.toLowerCase() === 'yes') {
      location.href='mailto:giraffewatchapp@gmail.com?subject=April is in Active Labor'
      document.getElementById('giraffe-event-result').style.display = 'block';
      ga('send', 'event', 'Labor Notification', 'Confirmed', location.href);
    } else {
      ga('send', 'event', 'Labor Notification', 'Unconfirmed', location.href);
    }
  }